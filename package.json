{
    "name": "bem-expand",
    "displayName": "BEM Expand",
    "description": "You already write BEM with SCSS and enjoy using `&` for parent selector. But you also want to use it in your HTML/JSX.",
    "version": "0.1.1",
    "publisher": "lukazakrajsek",
    "repository": {
        "type": "git",
        "url": "https://github.com/bancek/vscode-bem-expand"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://raw.githubusercontent.com/bancek/vscode-bem-expand/master/LICENSE.md"
        }
    ],
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "BEM",
        "HTML",
        "SCSS",
        "Emmet",
        "React"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.bemExpand",
            "title": "BEM Expand"
        }],
        "keybindings": [{
            "command": "extension.bemExpand",
            "key": "tab",
            "when": "config.emmet.triggerExpansionOnTab && editorTextFocus && !editorHasMultipleSelections && !editorHasSelection && !editorReadonly && !editorTabMovesFocus"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}